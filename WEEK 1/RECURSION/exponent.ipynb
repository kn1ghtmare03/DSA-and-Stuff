    "#optimization \n",
    "def exp(x,n):\n",
    "    if(n==0):\n",
    "        return 1\n",
    "    x1 = exp(x,n//2)\n",
    "    if(n%2==0):\n",
    "        return x1*x1\n",
    "    else:\n",
    "        return x*x1*x1\n",
    "exp(2,5)\n"
   ]
